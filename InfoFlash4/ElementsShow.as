package  {		import flash.display.MovieClip;		public class ElementsShow extends MovieClip	{		public var ElementsSList:Array;		public var info_text:String;		public var info_title:String;				public function ElementsShow(ELinfo:XML) 		{			this.ElementsSList = new Array();			var imglist:XMLList = ELinfo.imageMark.imageS;						this.info_title = ELinfo.imageMark.imageS.imageTitle;			trace(info_title);						this.info_text = ELinfo.imageMark.shortInfo;			trace(info_text);						for (var i=0; i<imglist.length(); i++)			{				var m = new elementsLoader(imglist[i]);				 addChild(m);							}								}	}	}