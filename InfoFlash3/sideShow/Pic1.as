package  {		import flash.display.MovieClip;	import flash.display.Loader;	import flash.net.URLRequest;		public class Pic1 extends MovieClip	{				private var original_x:int;		private var original_y:int;		private var original_scale:Number;		private var wholePics_img:Loader;				public function Pic1(makeup_info:XML) 		{			var rawImage:String = makeup_info.imgURL;			original_x = makeup_info.imageX;			original_y = makeup_info.imageY;			original_scale = makeup_info.scaled;						trace(rawImage);			this.wholePics_img = new Loader();			this.wholePics_img.load(new URLRequest(rawImage));			addChild(this.wholePics_img);						this.resetMe();		}		public function resetMe()		{			this.x = original_x;			this.y = original_y;			this.scaleX = this.scaleY = original_scale;					}	}	}