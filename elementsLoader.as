package  {		import flash.display.Loader;	import flash.display.MovieClip;	import flash.net.URLRequest;	import flash.events.MouseEvent;		public class elementsLoader extends MovieClip 	{				public var original_x:int;		public var original_y:int;				private var original_scale:Number;		private var makeup_img:Loader;		public function elementsLoader(element_info:XML) 		{			// constructor code						var rawImage:String = element_info.imgURL;			original_x = element_info.imageX;			original_y = element_info.imageY;			original_scale = element_info.scaled;						trace(rawImage);						this.makeup_img = new Loader();			this.makeup_img.load(new URLRequest(rawImage));			addChild(this.makeup_img);						this.resetMe();		}		public function resetMe()		{			this.x = original_x;			this.y = original_y;			this.scaleX = this.scaleY = original_scale;								}					}	}